{
  "extends": "../../tsconfig.base.json",
  "include": [
    "./",
    "src",
    "test",
  ],
  "compilerOptions": {
    "rootDir": "./",
    "baseUrl": "./",
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "composite": true,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": [
      "vite/client",
      "jest",
      "node"
    ]
  },
  "references": [
    {
      "path": "../antd"
    },
    {
      "path": "../chakra-ui"
    },
    {
      "path": "../core"
    },
    {
      "path": "../daisyui"
    },
    {
      "path": "../fluentui-rc"
    },
    {
      "path": "../mui"
    },
    {
      "path": "../react-bootstrap"
    },
    {
      "path": "../semantic-ui"
    },
    {
      "path": "../shadcn"
    },
    {
      "path": "../utils"
    },
    {
      "path": "../validator-ajv8"
    }
  ]
}
