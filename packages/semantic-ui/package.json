    "Jacques Nel <jacqueswho@gmail.com>"
  ],
  "keywords": [
    "Semantic UI",
    "react-jsonschema-form",
    "jsonschema",
    "json-schema",
    "json",
    "schema",
    "form",
    "react",
    "semantic-ui",
    "rjsf-semantic-ui"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjsf-team/react-jsonschema-form.git"
