  "description": "Ant Design theme, fields and widgets for react-jsonschema-form",
  "main": "dist/index.js",
  "module": "dist/antd.esm.js",
  "scripts": {
    "start": "tsdx watch",
    "build": "rimraf dist && tsdx build --format cjs,es,umd",
    "test": "tsdx test",
    "test:update": "tsdx test --u"
  },
  "files": [
    "dist"
  ],
  "engineStrict": false,
  "engines": {
    "node": ">=12"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "peerDependencies": {
    "@ant-design/icons": "^4.0.0",
    "@rjsf/core": "^4.2.0",
    "@rjsf/utils": "^4.2.0",
    "antd": "^4.0.0",
    "dayjs": "^1.8.0",
    "react": ">=16"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.0.0",
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.6",
    "@rjsf/core": "^4.2.0",
    "@rjsf/utils": "^4.2.0",
    "@rjsf/validator-ajv6": "^4.2.0",
    "antd": "^4.0.0",
    "atob": "^2.0.3",
    "babel-eslint": "^10.0.1",
    "dayjs": "^1.11.3",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.26.0",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^16.14.0",
    "rimraf": "^3.0.2",
    "tsdx": "^0.14.1"
