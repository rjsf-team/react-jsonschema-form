{
  "$schema": "https://json.schemastore.org/tailwindcss-config",
  "content": ["./src/**/*.{js,jsx,ts,tsx}"],
  "theme": {
    "extend": {}
  },
  "plugins": ["daisyui"],
  "daisyui": {
    "themes": true,
    "darkTheme": "dark",
    "base": true,
    "styled": true,
    "utils": true,
    "prefix": "",
    "logs": false,
    "themeRoot": ":root"
  }
}
