{
  "extends": "../../tsconfig.build.json",
  "compilerOptions": {
    "outDir": "./lib"
  },
  "files": [],
  "references": [
    {
      "path": "./src"
    }
  ],
  "tsc-alias": {
    "resolveFullPaths": true,
    "verbose": true,
    "replacers": {
      "lodash": {
        "enabled": true,
        "file": "lodashReplacer.cjs"
      },
      "ajv": {
        "enabled": true,
        "file": "ajvReplacer.cjs"
      }
    }
  }
}
