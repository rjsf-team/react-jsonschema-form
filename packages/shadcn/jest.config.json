{
  "testEnvironment": "jsdom",
  "setupFilesAfterEnv": ["../../testing/testSetup.ts", "<rootDir>/test/setup-jest-env.ts"],
  "transform": {
    "^.+\\.(ts|tsx|js|jsx)$": ["babel-jest", { "configFile": "./babel.config.json" }]
  },
  "transformIgnorePatterns": ["/node_modules/(?!(@x0k/json-schema-merge|deep-freeze-es6))"],
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/src/$1"
  }
}
