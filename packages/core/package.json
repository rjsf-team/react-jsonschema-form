{
  "name": "@rjsf/core",
  "version": "4.2.0",
  "description": "A simple React component capable of building HTML forms out of a JSON schema.",
  "scripts": {
    "build": "rimraf dist && tsdx build --format cjs,es,umd",
    "cs-check": "prettier -l \"{src,test}/**/*.[jt]s?(x)\"",
    "cs-format": "prettier \"{src,test}/**/*.[jt]s?(x)\" --write",
    "lint": "eslint src test",
    "precommit": "lint-staged",
    "publish-to-npm": "npm run build && npm publish",
    "start": "tsdx watch",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "test:update": "tsdx test --u",
    "test-coverage": "tsdx test --coverage"
  },
  "lint-staged": {
    "{src,test}/**/*.[jt]s?(x)": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "main": "dist/index.js",
  "module": "dist/core.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "engineStrict": false,
  "engines": {
    "node": ">=12"
  },
  "peerDependencies": {
    "react": ">=16 || >=17",
    "@rjsf/utils": "^4.2.0"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "nanoid": "^3.1.23",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.18.6",
    "@babel/plugin-transform-object-assign": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.6",
    "@rjsf/utils": "^4.2.0",
    "@rjsf/validator-ajv6": "^4.2.0",
    "@types/lodash": "^4.14.182",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "ajv": "^6.7.0",
    "atob": "^2.1.2",
    "chai": "^3.3.0",
    "eslint": "^8.19.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "html": "^1.0.0",
    "jsdom": "^16.5.3",
    "lint-staged": "^10.1.3",
    "mocha": "^5.2.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-portal": "^4.2.2",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "tsdx": "^0.14.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjsf-team/react-jsonschema-form.git"
  },
  "author": "Nicolas Perriault <nperriault@mozilla.com>",
  "contributors": [
    "Heath Chiavettone <heath.chiavettone@freenome.com"
  ],
  "keywords": [
    "react",
    "form",
    "json-schema"
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/rjsf-team/react-jsonschema-form",
  "publishConfig": {
    "access": "public"
  }
}
