{
  "extends": "../../../tsconfig.base.json",
  "include": [
    "./"
  ],
  "compilerOptions": {
    "rootDir": "./",
    "outDir": "../lib",
    "baseUrl": "../",
    "jsx": "react-jsx"
  },
  "references": [
    {
      "path": "../../utils"
    },
    {
      "path": "../../validator-ajv6"
    },
    {
      "path": "../../validator-ajv8"
    }
  ],
  "tsc-alias": {
    "resolveFullPaths": true,
    "verbose": true,
    "replacers": {
      "lodash": {
        "enabled": true,
        "file": "lodashReplacer.js"
      }
    }
  }
}
